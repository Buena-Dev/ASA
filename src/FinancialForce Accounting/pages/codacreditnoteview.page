<!-- codacreditnoteview -->
<apex:page standardController="c2g__codaCreditNote__c" extensions="c2g.CODAHelpLoaderController,c2g.CODACreditNoteController" action="{!maximunNumberOfLinesReached}">
	<script src="{!URLFOR($Resource.coda2go,'coda2go/web/script/lib.js')}" type="text/javascript"></script>
	<apex:stylesheet value="{!URLFOR($Resource.c2g__coda2go,'coda2go/web/css/style.css')}"/>
	<style>
	.extracollapsed
	{
		BACKGROUND-IMAGE: url( {!URLFOR($Resource.coda2go, 'coda2go/web/images/CollapsedExpanded.gif')});
	}

	.extraexpanded
	{
		BACKGROUND-IMAGE: url( {!URLFOR($Resource.coda2go, 'coda2go/web/images/CollapsedExpanded.gif')});
	}

	.extracollapsed_h
	{
		BACKGROUND-IMAGE: url( {!URLFOR($Resource.coda2go, 'coda2go/web/images/CollapsedExpanded.gif')});
	}

	.extraexpanded_h
	{
		BACKGROUND-IMAGE: url( {!URLFOR($Resource.coda2go, 'coda2go/web/images/CollapsedExpanded.gif')});
	}

	.pbError
	{
		background-image:url({!URLFOR($Resource.coda2go, 'coda2go/web/images/pbErrorIcon.gif')});
	}
	.pbSuccess
	{
		background-image:url({!URLFOR($Resource.coda2go, 'coda2go/web/images/pbSuccessIcon.gif')});
	}

	.extracollapsed
	{
		vertical-align:top;
		position:relative;
		top:-2px;
	}
	</style>
	<script>
		var CODA2GoButtons = new CODA2GoPage();
		var CODA2GoBtnSaveValue = "{!$Label.codaPageCreditNoteViewProcessing}";
	</script>
	<apex:form onsubmit="if(CODA2GoButtons.disableSaveButtons){ return CODA2GoButtons.disableSaveButtons(); }">
		<!-- Header -->
		<apex:sectionHeader title="{!$Label.c2g__codapagecreditnoteviewcreditnotedetail}" subtitle="{!viewstate.creditNote.dto.Name}" help="{!redirectForHelp1025}"/>
		<!-- Back to CreditNotes -->
		<DIV class="goBreadcrumb">
		&nbsp;&#171;<a href="{!URLFOR($Action.codaCreditNote__c.Tab,$ObjectType.codaCreditNote__c)}">{!$Label.codaPageCreditNoteViewBackToListCredit}</a>
		</DIV>
		<!-- CreditNote Detail -->
		<apex:pageBlock title="{!$Label.c2g__codapagecreditnoteviewcreditnotedetail2}" id="creditNotepb">
			<!-- Buttons -->
			<apex:pageBlockButtons >
				<!-- Hidden Buttons -->
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewamend}" id="amend_h" action="{!gotoAmendDocument}" rendered="{!viewstate.creditNote.canAmendDocument}" styleclass="hiddensubmit"/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewedit}" id="edit_h" action="{!edit}" rendered="{!viewstate.creditNote.canEdit}" styleclass="hiddensubmit"/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewdiscard}" id="discard_h" action="{!gotoDiscard}" rendered="{!viewstate.creditNote.canDiscard}" styleclass="hiddensubmit"/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpost}" id="post_h" action="{!post}" rendered="{!viewstate.creditNote.canPost}" styleclass="hiddensubmit"/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpost}" id="postVal_h" action="{!postVal}" rendered="{!viewstate.creditNote.canPostVal}" styleclass="hiddensubmit"/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpostandmatch}" id="postMatch_h" onclick="return;" action="{!postAndMatch}" rendered="{!viewstate.creditnote.canPostandMatch}" styleClass="hiddensubmit" />
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewmatch}" id="match_h" onclick="return;" action="{!postAndMatch}" rendered="{!viewstate.creditnote.canMatch}" styleClass="hiddensubmit" />
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewemailpdf}" id="emailPDF_h" onclick="return;" action="{!email}" rendered="{!viewstate.creditNote.canPrint}" styleclass="hiddensubmit"/>

				<!-- End of Hidden Buttons -->
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewamend}" id="amend" rendered="{!viewstate.creditNote.canAmendDocument && ($Permission.c2g__SalesCreditNoteAmend || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.amend_h}').click();return false;" rerender=""/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewedit2}" id="edit" rendered="{!viewstate.creditNote.canEdit && ($Permission.c2g__SalesCreditNoteEdit || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.edit_h}').click();return false;" rerender=""/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewdiscard2}" id="discard" rendered="{!viewstate.creditNote.canDiscard && ($Permission.c2g__SalesCreditNoteDiscard || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.discard_h}').click();return false;" rerender=""/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpost2}" id="post" rendered="{!viewstate.creditNote.canPost && ($Permission.c2g__SalesCreditNotePost || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.post_h}').click();return false;" rerender="post, postVal"/>
				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpost2}" id="postVal" rendered="{!viewstate.creditNote.canPostVal && ($Permission.c2g__SalesCreditNotePost || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.postVal_h}').click();return false;" rerender="post, postVal"/>

				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpreview}" id="preview" onClick="integrationRuleCheck('{!viewstate.printUrl}');" oncomplete="if(CODA2GoButtons) CODA2GoButtons.enableSaveButtons();" rendered="{!viewstate.creditNote.canPrint && ($Permission.c2g__SalesCreditNotePreview || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" reRender="messages"/>

				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewprint2}" id="print" action="{!print}" oncomplete="CODA2GoButtons.enableSaveButtons(); if(isValidCompany) integrationRuleCheck('{!viewstate.printUrl}');" rendered="{!viewstate.creditNote.canPrint && ($Permission.c2g__SalesCreditNotePrint || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" reRender="jsCompanyMode,messages"/>

				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewviewpdf}" id="viewPDF" onClick="integrationRuleCheck('{!viewstate.pdfUrl}');" oncomplete="if(CODA2GoButtons) CODA2GoButtons.enableSaveButtons();" rendered="{!viewstate.creditNote.canViewPDF && ($Permission.c2g__SalesCreditNoteViewPDF || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" reRender="messages"/>

				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewprintpdf2}" id="printPDF" action="{!print}" oncomplete="CODA2GoButtons.enableSaveButtons(); if(isValidCompany) integrationRuleCheck('{!viewstate.pdfUrl}');" rendered="{!viewstate.creditNote.canPrint && ($Permission.c2g__SalesCreditNotePrintPDF || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" reRender="jsCompanyMode,messages"/>


				 <apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewemailpdf2}" id="emailPDF" rendered="{!viewstate.creditNote.canPrint && ($Permission.c2g__SalesCreditNoteEmailPDF || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="return integrationRuleCheckWithButton('{!$Component.emailPDF_h}');" rerender=""/>

				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewpostandmatch}" id="postMatch" rendered="{!viewstate.creditNote.CanPostandMatch && (($Permission.c2g__SalesCreditNoteMatch && $Permission.c2g__SalesCreditNotePost) || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.postMatch_h}').click();return false;" rerender="" />

				<apex:commandButton value="{!$Label.c2g__codapagecreditnoteviewmatch}" id="match" rendered="{!viewstate.creditNote.CanMatch && ($Permission.c2g__SalesCreditNoteMatch || $Setup.codaAccountingSettings__c.DisableCustomPermissions__c)}" onclick="document.getElementById('{!$Component.match_h}').click();return false;" rerender="" />

				<script>
					var CODA2GoButtonArray = "";

	                if(document.getElementById("{!$Component.amend}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.amend}";

	                if(document.getElementById("{!$Component.edit}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.edit}";

	                if(document.getElementById("{!$Component.discard}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.discard}";

   	                if(document.getElementById("{!$Component.post}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.post}";

	                if(document.getElementById("{!$Component.postVal}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.postVal}";

	                if(document.getElementById("{!$Component.preview}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.preview}";

	                if(document.getElementById("{!$Component.print}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.print}";

	                if(document.getElementById("{!$Component.viewPDF}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.viewPDF}";

	                if(document.getElementById("{!$Component.printPDF}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.printPDF}";

					if(document.getElementById("{!$Component.emailPDF}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.emailPDF}";

					if(document.getElementById("{!$Component.postMatch}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.postMatch}";

					if(document.getElementById("{!$Component.match}"))
	                	CODA2GoButtonArray = CODA2GoButtonArray + ',' + "{!$Component.match}";

					var myCODA2GoButtonArray = CODA2GoButtonArray.substr(1).split(",");
					
	                CODA2GoButtons.initButtons(myCODA2GoButtonArray);
				</script>
			</apex:pageBlockButtons>
			<!-- Messages -->
			<apex:outputPanel id="messages">
				<apex:pageMessage title="{!$Label.c2g__documentreadytopostwarning}" severity="warning" strength="2" rendered="{!viewstate.IsReadyPost}"/>
				<apex:include pageName="c2g__codamulticompanymodewarning"  rendered="{!viewstate.isMultiCompanyMode}"/>
				<apex:include pageName="c2g__codacompanymismatchwarning"  rendered="{!viewstate.ownerCmpErr}"/> 
				<apex:pageMessages id="messagePanel"/>
				<apex:pageMessage title="{!$Label.c2g__documentreadytopostandmatchwarning}" severity="warning" strength="2" rendered="{!viewstate.IsReadyPostAndMatch}"/> 
				<apex:pagemessages id="warningPanel" rendered="{!showWarningPanel}"/>
			</apex:outputPanel>

			<!-- Account Detail -->
			<apex:pageBlockSection title="{!$Label.c2g__codapagecreditnoteviewaccountdetail}">

				<apex:pageBlockSectionItem >
					<apex:outputLabel value="{!$ObjectType.c2g__codaCreditNote__c.fields.c2g__Account__c.label}"/>
					<apex:outputPanel >
						<apex:outputField value="{!viewstate.creditNote.dto.c2g__Account__c}"/>
						<c:AnalysisPopup containerClass="headerexpander" tooltip="{!$Label.c2g__codapagecreditnoteviewtoggleanalysisar}" id="popupHeader" allowConcurrentPopups="true">
							<c:SubAnalysis record="{!viewstate.creditNote.dto}" title="{!$Label.c2g__codaanalysis}" readOnly="true"/>
									
							<!-- CUSTOM FIELDS HEADER -->
							<apex:pageBlockSection title="{!$Label.c2g__codapagecreditnoteviewadditionalinformation}" id="headerCustomBlock" collapsible="false" columns="1" rendered="{!viewstate.DynamicFieldsInfoTable.size > 0}">
								<apex:dataTable id="dtDynamicFields" value="{!viewstate.DynamicFieldsInfoTable}" var="fieldInfo" rowClasses="odd,even" headerClass="listheader" styleClass="list detailListnoPadding" border="0" onRowMouseOver="if (window.hiOn){hiOn(this);}" onRowMouseOut="if (window.hiOff){hiOff(this);}">
									<apex:column styleClass="widthauto">
										<apex:outputText value="{!fieldInfo.DynamicFieldController.DynamicFieldInfo.label}"/>
									</apex:column>
									<apex:column styleClass="widthauto">
										<!--
										The implementation using new feature Dynamic Binding in VF would be much easier, efficient and user friendly. We would not need any custom component
										<apex:repeat value="{!myFieldList}" var="f">
											<apex:outputField value="{!account[f]}"/>
										</apex:repeat>
										 -->
										<c:CODADynamicField m_fieldinfo="{!fieldInfo.DynamicFieldController.DynamicFieldInfo}" m_mode="VIEW" m_objectc="codaCreditNote__c"/>
									</apex:column>
					        	</apex:dataTable>
	                        </apex:pageBlockSection>
						</c:AnalysisPopup>
					</apex:outputPanel>
				</apex:pageBlockSectionItem>

				<apex:pageBlockSectionItem >
					<apex:outputLabel value="{!$Label.c2g__codapageopportunityconvertopportunity}"/>
					<apex:outputField value="{!viewstate.creditNote.dto.c2g__Opportunity__c}"/>
				</apex:pageBlockSectionItem>


				<!-- Below pageBlockSectionItem rendered depending on Tax Method -->

				<apex:pageBlockSectionItem rendered="{!viewstate.isSalesAndUse}">
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewtaxcodes}"/>

					<apex:outputPanel styleClass="holder">
						<apex:outputPanel >
							<apex:outputText value="{!viewstate.creditNote.taxCodesAsString}"/>
						</apex:outputPanel>
						<a href="#" onclick="CODA.toggleHeaderExpand(this,'myex1');return false;" title="{!$Label.codaPageCreditNoteViewToggleTaxCodesAr2}"><img src="/s.gif" width="16" height="16" class="extracollapsed_h" title="{!$Label.codaPageCreditNoteViewToggleTaxCodesAr2}"/></a>
						<DIV id="myex1" class="headerexpander">
								<apex:pageBlockSection title="{!$Label.c2g__codapagecreditnoteviewtaxcodes2}" collapsible="false" columns="1">
									<apex:panelGrid width="100%" columns="3" border="0" columnClasses="labelCol first, data2Col first,data2Col">
										<apex:outputLabel value="{!$ObjectType.c2g__codaCreditNote__c.fields.c2g__TaxCode1__c.label}"/>
										<apex:outputPanel ><apex:outputField value="{!viewstate.creditNote.dto.c2g__TaxCode1__c}"/></apex:outputPanel>
										<apex:outputPanel ><apex:outputText value="{!viewstate.creditNote.TaxRate1}"/></apex:outputPanel>
										<apex:outputLabel value="{!$ObjectType.c2g__codaCreditNote__c.fields.c2g__TaxCode2__c.label}"/>
										<apex:outputPanel ><apex:outputField value="{!viewstate.creditNote.dto.c2g__TaxCode2__c}"/></apex:outputPanel>
										<apex:outputPanel ><apex:outputText value="{!viewstate.creditNote.TaxRate2}"/></apex:outputPanel>
										<apex:outputLabel value="{!$ObjectType.c2g__codaCreditNote__c.fields.c2g__TaxCode3__c.label}"/>
										<apex:outputPanel ><apex:outputField value="{!viewstate.creditNote.dto.c2g__TaxCode3__c}"/></apex:outputPanel>
										<apex:outputPanel ><apex:outputText value="{!viewstate.creditNote.TaxRate3}"/></apex:outputPanel>
									</apex:panelGrid>
								</apex:pageBlockSection>
						</DIV>

					</apex:outputPanel>
				</apex:pageBlockSectionItem>



				<apex:pageBlockSectionItem >
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewinvoicenumber}"/>
					<apex:outputField value="{!viewstate.creditNote.dto.c2g__Invoice__c}"/>
				</apex:pageBlockSectionItem>
				<apex:pageBlockSectionItem >
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewbillingaddress}"/>
					<apex:outputPanel >
					<apex:outputText value="{!viewstate.creditNote.billingAddress.fullAddress}"/>&nbsp;
					</apex:outputPanel>
				</apex:pageBlockSectionItem>
				<apex:pageBlockSectionItem >
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewshippingaddress}"/>
					<apex:outputPanel >
					<apex:outputText value="{!viewstate.creditNote.shippingAddress.fullAddress}"/>&nbsp;
					</apex:outputPanel>
				</apex:pageBlockSectionItem>
				<apex:pageBlockSectionItem rendered="{!viewstate.creditNote.canPrint}">
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewinvoiceemail}"/>
					<apex:outputText value="{!viewstate.creditNote.creditNote.account.Dto.c2g__CODAInvoiceEmail__c}"/>

				</apex:pageBlockSectionItem>



			</apex:pageBlockSection>

			<!-- CreditNote Detail -->
			<apex:pageBlockSection title="{!$Label.c2g__codapagecreditnoteviewcreditnotedetail3}">
				<apex:outputField value="{!viewstate.creditNote.dto.Name}"/>
				<apex:outputField value="{!viewstate.creditNote.dto.c2g__Transaction__c}"/>
				<apex:outputField value="{!viewstate.creditNote.dto.c2g__CreditNoteDate__c}"/>

				<apex:outputField id="period" value="{!viewstate.creditNote.dto.c2g__Period__c}"/>

				<apex:outputField id="invoiceDate" value="{!viewstate.creditNote.dto.c2g__InvoiceDate__c}" rendered="{!!viewstate.isConvertedFromInvoice}"/>
				<apex:pageBlockSectionItem rendered="{!viewstate.isConvertedFromInvoice}">
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewinvoicedate}"/>
					<apex:outputField value="{!viewstate.creditNote.creditNote.invoice.dto.c2g__InvoiceDate__c}"/>
				</apex:pageBlockSectionItem>

				<apex:pageBlockSectionItem >
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewcustomerreferenc}"/>
					<apex:outputPanel >
						<apex:outputField value="{!viewstate.creditNote.dto.c2g__CustomerReference__c}"/>&nbsp;
					</apex:outputPanel>
				</apex:pageBlockSectionItem>
				<apex:outputField value="{!viewstate.creditNote.dto.c2g__DueDate__c}"/>
				<apex:outputField value="{!viewstate.creditNote.dto.c2g__CreditNoteReason__c}"/>

				<apex:pageBlockSectionItem id="currencyblock">
					<apex:outputLabel value="{!$Label.c2g__codapagecreditnoteviewcreditnotecurren}"/>
					<apex:outputPanel >
						<apex:outputText value="{!viewstate.creditNote.creditNote.creditNoteCurrency.name}"/>
						<c:ExchangeRateOverrides id="currencyRates" rateData="{!viewstate.creditNote.exchangeRateData}" readOnly="true"/>
					</apex:outputPanel>
				</apex:pageBlockSectionItem>

				<apex:outputField value="{!viewstate.creditNote.dto.c2g__CreditNoteDescription__c}"/>
			</apex:pageBlockSection>

			<!-- CreditNote Line Items -->
			<apex:pageBlockSection title="{!$Label.c2g__codapagecreditnoteviewcreditnotelineit}" columns="1">
				<apex:dataTable id="dtLineItems" value="{!viewstate.creditNote.lineItems}" var="lineItem" rowClasses="odd,even" styleClass="list" headerClass="listheader" border="0" onRowMouseOver="if (window.hiOn){hiOn(this);}" onRowMouseOut="if (window.hiOff){hiOff(this);}">
					<apex:column styleClass="widthlinenumbers">
						<apex:facet name="header">&nbsp;</apex:facet>
						<apex:outputPanel >
							<span class="noWrap">
								<apex:outputField value="{!lineItem.dto.c2g__LineNumber__c}"/>
								<c:AnalysisPopup containerClass="extra" tooltip="{!$Label.c2g__codapagecreditnoteviewtogglenotesandan}" id="popupLine">
									<apex:pageBlockSection title="{!$Label.c2g__alternateaccount}" collapsible="false" columns="2" html-sac="1" rendered="{!allowLocalGLA && viewstate.EnableOverrideProductGLA}">
								        <apex:pageBlockSectionItem html-sac="1" rendered="{!showLocalGLA}">	
								        	<apex:outputLabel value="{!$ObjectType.c2g__codaCreditNoteLineItem__c.fields.c2g__GeneralLedgerAccount__c.label}" styleClass="fieldLabel"/>
								        	<apex:outputPanel >
												<span class="popupDataFieldOutput">
													<apex:outputField value="{!lineItem.dto.c2g__GeneralLedgerAccount__c}"/>
												</span>
											</apex:outputPanel>	
								        </apex:pageBlockSectionItem>
								        <apex:pageBlockSectionItem html-sac="1" rendered="{!allowLocalGLA && !showLocalGLA}">	
								        	<apex:outputLabel value="{!$ObjectType.c2g__codaCreditNoteLineItem__c.fields.c2g__LocalGLA__c.label}" styleClass="fieldLabel"/>
								        	<apex:outputPanel >
												<span class="popupDataFieldOutput">
													<apex:outputField value="{!lineItem.dto.c2g__LocalGLA__c}"/>
												</span>
											</apex:outputPanel>	
								        </apex:pageBlockSectionItem>
									</apex:pageBlockSection>
									<apex:outputPanel layout="block" styleClass="headerdetailsview">

										<c:SubAnalysis record="{!lineItem.dto}" title="{!$Label.c2g__codanotesandanalysis}" readOnly="true" intercompany-detail="true" lineNumber="{!lineItem.dto.c2g__LineNumber__c}"/>

										<!-- CUSTOM FIELDS LINE ITEM -->
										<apex:pageBlockSection title="{!$Label.c2g__codapagecreditnoteviewadditionalinformation}" id="customFieldBlock" collapsible="false" columns="2" rendered="{!lineItem.DynamicFieldsInfoTableLI.size > 0}">
											<apex:dataTable id="dtDynamicFieldLineItems" value="{!lineItem.DynamicFieldsInfoTableLI}" var="fieldLI" rowClasses="odd,even" headerClass="listheader" styleClass="list detailListnoPadding" border="0" onRowMouseOver="if (window.hiOn){hiOn(this);}" onRowMouseOut="if (window.hiOff){hiOff(this);}">
												<apex:column styleClass="widthauto">
													<apex:outputText value="{!fieldLI.DynamicFieldController.dynamicFieldInfo.label}"/>
									            </apex:column>
												<apex:column styleClass="widthauto">
													<!--
													The implementation using new feature Dynamic Binding in VF would be much easier, efficient and user friendly. We would not need any custom component
													<apex:repeat value="{!myFieldList}" var="f">
														<apex:outputField value="{!account[f]}"/>
													</apex:repeat>
													 -->
													<c:CODADynamicField m_fieldinfo="{!fieldLI.DynamicFieldController.DynamicFieldInfo}" m_mode="VIEW" m_objectc="codaCreditNoteLineItem__c"/>
										        </apex:column>
									   		</apex:dataTable>
										</apex:pageBlockSection>
									</apex:outputPanel>
								</c:AnalysisPopup>
							</span>
						</apex:outputPanel>
					</apex:column>
					<apex:column >
						<apex:facet name="header">{!$ObjectType.c2g__codaCreditNoteLineItem__c.fields.c2g__Product__c.label}</apex:facet>
						<apex:outputField value="{!lineItem.dto.c2g__Product__c}"/>
					</apex:column>
					<apex:column rendered="{!!showLocalGLA && viewstate.EnableOverrideProductGLA}">
						<apex:facet name="header">{!$ObjectType.c2g__codaCreditNoteLineItem__c.fields.c2g__GeneralLedgerAccount__c.label}</apex:facet>
						<apex:outputField value="{!lineItem.dto.c2g__GeneralLedgerAccount__c}"/>
					</apex:column>
					
					<apex:column rendered="{!showLocalGLA && viewstate.EnableOverrideProductGLA}">
                        <apex:facet name="header">{!$ObjectType.c2g__codaCreditNoteLineItem__c.fields.c2g__LocalGLA__c.label}</apex:facet>
                        <apex:outputField value="{!lineItem.dto.c2g__LocalGLA__c}"/>
                    </apex:column>
                    
					<apex:column >
						<apex:facet name="header">{!$ObjectType.product2.fields.ProductCode.label}</apex:facet>
						<apex:outputText value="{!lineItem.ProductCode}"/>
					</apex:column>
