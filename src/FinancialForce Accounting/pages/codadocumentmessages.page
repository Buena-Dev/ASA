<!-- codadocumentmessages -->
<apex:page controller="c2g.CODADocumentMessagesController">
	<apex:sectionHeader title="{!$Label.c2g__codadocumentmessagepagetitle}" subtitle="{!$Label.c2g__codadocumentmessagepagesubtitle}"/>
	<apex:form >
		<apex:pageBlock mode="detail">
			<apex:pageMessages />
            <apex:pageBlockButtons location="bottom">
                <apex:outputpanel id="buttonPanel">
                    <apex:commandButton id="back" value="{!$Label.c2g__back}" action="{!back}"/>
                </apex:outputpanel>
            </apex:pageBlockButtons>
        </apex:pageBlock>
	</apex:form>
</apex:page>