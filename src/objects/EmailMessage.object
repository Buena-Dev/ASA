    <validationRules>
        <fullName>Restrict_Send_Email_From</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status, &apos;3&apos;),NOT(ISNULL(ParentId)),LOWER(FromAddress) &lt;&gt; &quot;askasa@asa.org&quot;, LOWER(FromAddress) &lt;&gt; &quot;salt@saltmoney.org&quot;, LOWER(FromAddress) &lt;&gt; &quot;support@asa.org&quot;)</errorConditionFormula>
        <errorDisplayField>BccAddress</errorDisplayField>
        <errorMessage>Please select the correct From address, which must be one of the following: askasa@asa.org, salt@saltmoney.org, or support@asa.org</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Send_Email_From_askasa_Reply</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status, &apos;3&apos;), NOT(ISNULL(ParentId)), NOT(ISNULL(Parent.Email2Case_TO_Address__c)), LOWER(FromAddress) &lt;&gt; LOWER(Parent.Email2Case_TO_Address__c), Parent.Owner:Queue.DeveloperName = &quot;Askasa_asa_org_Email&quot;)</errorConditionFormula>
        <errorMessage>Please select the correct From address: askasa@asa.org</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Send_Email_From_saltmoney_Reply</fullName>
